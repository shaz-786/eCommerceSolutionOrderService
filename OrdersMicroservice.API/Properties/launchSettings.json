{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "MONGODB_HOST": "localhost", // this will look at mongo container running at port 27017
        "MONGODB_PORT": "27017",
        "MONGODB_DATABASE": "OrdersDatabase",
        "UsersMicroserviceName": "localhost",
        "UsersMicroservicePort": "9090",
        "ProductsMicroserviceName": "localhost",
        "ProductsMicroservicePort": "8080"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5027",
      "launchUrl": "swagger"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "MONGODB_HOST": "localhost",
        "MONGODB_PORT": "27017",
        "MONGODB_DATABASE": "OrdersDatabase",
        "UsersMicroserviceName": "localhost",
        "UsersMicroservicePort": "9090",
        "ProductsMicroserviceName": "localhost",
        "ProductsMicroservicePort": "8080"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7155;http://localhost:5027",
      "launchUrl": "swagger"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "MONGODB_HOST": "localhost",
        "MONGODB_PORT": "27017",
        "MONGODB_DATABASE": "OrdersDatabase",
        "UsersMicroserviceName": "localhost",
        "UsersMicroservicePort": "9090",
        "ProductsMicroserviceName": "localhost",
        "ProductsMicroservicePort": "8080"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "MONGODB_HOST": "localhost",
        "MONGODB_PORT": "27017",
        "MONGODB_DATABASE": "OrdersDatabase",
        "UsersMicroserviceName": "localhost",
        "UsersMicroservicePort": "9090",
        "ProductsMicroserviceName": "localhost",
        "ProductsMicroservicePort": "8080"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:59180",
      "sslPort": 44373
    }
  }
